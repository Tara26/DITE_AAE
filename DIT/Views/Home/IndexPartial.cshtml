        margin: 5px;
        cursor: pointer;
    }

    body {
        padding-right: 0px !important;
    }

    .datagrid {
        overflow-y: scroll;
        height: 800px;
    }

    .table > thead:first-child > tr:first-child > th, .table > caption + thead > tr:first-child > td, .table > colgroup + thead > tr:first-child > td, .table > thead:first-child > tr:first-child > td {
