    var myIndex = 0;
    carousel();

    function carousel() {
        var i;
        var x = document.getElementsByClassName("mySlides");
        for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }
        myIndex++;
        if (myIndex > x.length) { myIndex = 1 }
        if (x[myIndex - 1] != null) {
            x[myIndex - 1].style.display = "block";
        }
        setTimeout(carousel, 2000); // Change image every 2 seconds
    }

    $("#btnSubmit").click(function (e) {
        var isValid = 1;
        var userName = $('#txtUserName').val();
        if (userName == "" || userName == null) {
            bootbox.alert("Please enter User Name");
            isValid = 0;
        }
        if (isValid) {
            //	$("#txtUserName").val(hashSHA256(userName));
            var userPassword = $('#txtPassword').val();

            if (userPassword == "" || userPassword == null) {
                bootbox.alert("Please enter User Password");
                isValid = 0;
            }
            $("#HDNpwd").val(userPassword);
            $("#txtPassword").val(hashSHA256(userPassword));
            userPassword = $('#txtPassword').val();
        }
        if (isValid) {
            
            //var CapturaText = document.getElementById("CaptchaImg").value;
            var CapturaText = document.getElementById("myCaptcha").innerHTML.replace(/\s/g, '');
            var EnteredCapturaText = $('#captchaText').val();

            if (CapturaText != EnteredCapturaText) {
                bootbox.alert("Please enter valid Captcha.");
                isValid = 0;
                $("#captchaText").val("");
                $("#txtPassword").val("");
            }
        }
        var OriginalPwdCheck = RandomWithPassword($("#HDNpwd").val());
        //alert(userPassword);
        if (isValid == 1) {
            $('#loading').show();
            $.ajax({
                url: "/Home/Index",
                datatype: "json",
                type: "POST",
                data: {
                    UserName: userName,
                    Password: userPassword,
                    HDNpwd: OriginalPwdCheck

                },
                success: function (result) {
                    $('#loading').hide();
                    if (result.success != false) {
                        if (result.subdeptCount > 1) {
                            GetSubDepartments();
                            GetLoginRoles();
                            $('#reg').modal('hide');
                            $('#ChangePassword').modal('hide');
                            $('#RoleModal').modal({ backdrop: 'static', keyboard: false });
                            $('#RoleModal').modal('show');
                        }
                        else {
                            $('#reg').modal('hide');
                            $('#ChangePassword').modal('hide');
                            window.location.href = "/Home/Index1";
                            $('#switchbtn').hide();
                        }
                    }
                    else {
                        //alert("3");
                        $('#txtUserName').val('');
                        $('#txtPassword').val('');
                        $("#captchaText").val('');
                       // loadCaptcha();
                        fngetcaptcha();
                        bootbox.alert(result.message);
                    }
                }, error: function (result) {

                    $('#txtUserName').val('');
                    $('#txtPassword').val('');
                    $("#captchaText").val('');

                    bootbox.alert(result.message);
                }
            });

        }
    });
    function Get_Notification_PublishDetails() {

        $.ajax({
            url: "/Home/Get_Notification_PublishDetails",
            datatype: "json",
            type: "GET",
            success: function (response) {
                if (response != null) {
                    for (let i = 0; i < response.length; i++) {
                        if (response[i].np_module_id == 1) {
                            $('#NotiPublishIndexTrade').show();
                        }
                        if (response[i].np_module_id == 2) {
                            $('#NotifPublishIndexInstitute').show();
                        }
                        if (response[i].np_module_id == 3) {
                            $('#NotiPublishIndexitiInstituteList').show();
                        }
                        if (response[i].np_module_id == 4) {
                            $('#NotiPublishfnAdmissionNotifications').show();
                            //$('#AdmissionNotModel').show();
                        }
                    }
                }
            }
        });
    }


