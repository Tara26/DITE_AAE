<style>
    table.dataTable thead tr {
        background-color: cornflowerblue;
        color: white;
    }

    .taluk {
        display: none;
    }

    .buttons-html5 {
        border-radius: 5px;
        height: 40px;
        width: 100px;
        background: cornflowerblue;
    }

    .headetitle {
        text-align: center;
        background-color: cornflowerblue;
        color: white;
        border-radius: 5px;
        height: 35px;
    }

    .tooltip {
        position: relative;
        display: inline-block;
        border-bottom: 1px dotted black;
    }

    .tooltiptext {
        visibility: hidden;
        width: 200px;
        background-color: black;
        color: #fff;
        text-align: center;
        border-radius: 6px;
        padding: 5px 0;
        /* Position the tooltip */
        position: absolute;
        z-index: 1;
    }

    #viewbtn:hover .tooltiptext {
        visibility: visible;
    }

    .buttons-html5 {
        width: 180px;
        padding: 5px;
        border: 1px solid grey;
        margin: 5px;
        cursor: pointer;
    }

    table th {
        text-align: center;
        font-size: smaller;
    }
</style>


