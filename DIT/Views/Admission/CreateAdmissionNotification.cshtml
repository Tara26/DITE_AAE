<script type="text/javascript">
    var selectedTab = '@Model.selectTab';
    if (selectedTab != null) {
        if (selectedTab == 0) {
            $('.nav-tabs li:eq(0) a').tab('show')
        }
        else if (selectedTab == 1) {
            $('.nav-tabs li:eq(1) a').tab('show')
        }
        else if (selectedTab == 2) {
            $('.nav-tabs li:eq(2) a').tab('show')
        }
    }

    var statusId = '@Model.StatusId';
    if (statusId != null) {
        if ($("#Exam_Notif_Id").val() != 0) {
            if (statusId == 107) {
                $("#createNotification1").val("Modify");
                $("#notif_date").val("");
            }
            else
                $("#createNotification1").val("Update");
        }
    }

   if ('@TempData["Saved"]' != "") {
        if ('@TempData["Saved"]' == "Saved") {

            var num = $('#notifNumber').val();
            bootbox.alert({
                message: "</br> <b> Case Worker </b> has Created Admission Notification No : <b>" + num + "</b> Successfully ",
                callback: function () {
                    fnRefreshScreenData();
                }
            });
        }
        else if ('@TempData["Saved"]' == "Draft") {
            var num = $('#notifNumber').val();
            bootbox.alert("</br> <b> Case Worker </b> has saved as Draft Admission Notification No : <b>" + num + "</b> Successfully");
        }
        else if ('@TempData["Saved"]' == "Updated") {
            var num = $('#notifNumber').val();
            bootbox.alert({
                message: "</br> <b> Case Worker </b> has Updated Admission Notification No : <b>" + num + "</b> Successfully ",
                callback: function () {
                    fnRefreshScreenData();
                }
            });
        }
        else if ('@TempData["Saved"]' == "Modified") {
            var num = $('#notifNumber').val();
            bootbox.alert({
                message: "</br> <b> Case Worker </b> has Modified Admission Notification No : <b>" + num + "</b> Successfully ",
                callback: function () {
                    fnRefreshScreenData();
                }
            });
        }
        else if ('@TempData["Saved"]' == "Failed") {
            bootbox.alert("</br> Failed to Save/Update data");
        }
        else if ('@TempData["Saved"]' == "Invalid") {
            bootbox.alert("</br> Some of the fields are invalid, please select valid data.");
        }
        else if ('@TempData["Saved"]' == "exist") {
            var num = $('#notifNumber').val();
            bootbox.alert("</br> Admission Notification No : <b>" + num + "</b> already exists");
        }
    }

    function changeTab1() {
        ClearFields1();
        if (selectedTab != null) {
            if (selectedTab == 0) {
                $('.nav-tabs li:eq(1) a').tab('show')
            }
            else if (selectedTab == 1) {
                $('.nav-tabs li:eq(1) a').tab('show')
            }
        }
    }
    function ClearFields1() {
        $('#ddlLang').val(1);
        $('#notifNumber').val('');
        $('#NotifDesc').val('');
        $('#courseType').val('');
        $("#applicantType").val('');
        document.getElementById("#Notificationfile").value = null;
        $("#UploadNotifFile-Info").text('');
    }
</script>

