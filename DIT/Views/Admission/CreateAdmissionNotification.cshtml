<script>
    $(document).ready(function () {
        $('#createnotifi').click(function () {
            ClearFields1();
        });

        let SaveNotification = document.getElementById('createNotification1');
        SaveNotification.addEventListener('click', (ev) => {
            ev.preventDefault();
            var data = new FormData();
            if (ValidateData() == true) {
                $('#CourseTypeErr').text('');
                $('#applicantTypeErr').text('');
                $('#NotificationNumErr').text('');
                $("#notif_dateErr").text('');
                var isValid = true;
                if ($('#courseType').val() == '') {
                    $('#CourseTypeErr').text('select course type');
                    isValid = false;
                }

                if ($('#notifNumber').val() == '') {
                    $('#NotificationNumErr').text('Enter the notification number');
                    isValid = false;
                }
                var notifDate = $("#notif_date").val().split('-');
                if (isNaN(new Date(notifDate[0], notifDate[1] - 1, notifDate[2]))) {
                    $("#notif_dateErr").text("Enter a valid Notification Date");
                    isValid = false;
                }

                if ($('#applicantType').val() == '') {
                    $('#applicantTypeErr').text('select Notification type');
                    isValid = false;
                }
                if (isValid) {
                    if (!$('#Notificationfile').val()) {
                        bootbox.alert("<br><br>Please upload the Notification Doc File");
                        isValid = false
                    }
                    else {
                        var SAFile = $("#Notificationfile").get(0).files;
                        if (SAFile.length > 0) {
                            files = checkValidationForFileSize(SAFile);
                            if (files == true) {
                                data.append("UploadPdf", SAFile[0]);
                            }
                            else {
                                isValid = false;
                            }
                        }
                    }
                }
                if (isValid == true) {

