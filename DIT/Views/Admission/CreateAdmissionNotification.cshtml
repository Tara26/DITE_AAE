        var fileExists = fileTypes.includes(type);

            if (fileExists == true) {
            var filesize = ((file1[0].size / 1024) / 1024).toFixed(5);
            if (filesize > 1) {
                bootbox.alert("Please upload a file of size less than 1 MB.");
                return false;
            }
            else {
                return true;
            }
        }
        else {
            bootbox.alert("Please upload a valid file");
            return false;
        }

    }
