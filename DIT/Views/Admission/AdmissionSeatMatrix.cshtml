                            <div class="col-sm-5">
                                <label>Course Type:</label>
                            </div>
                            <div class="col-sm-7">
                                <select class="form-control" style="width:100px;" id="CourseType2"></select>
                            </div>
                        </div>

                        <div class="col-sm-2">
                            <div class="col-sm-5">
                                <label>Division:</label>
                            </div>
                            <div class="col-sm-7">
                                <select class="form-control" style="width:100px;" id="divisionDdp" onchange="getfilter('divisionDdp',1)" @*onchange="GetDistrictData('divisionDdp','districtDdp1','1')"*@></select>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="col-sm-5">
                                <label>District:</label>
                            </div>
                            <div class="col-sm-7">
                                <select class="form-control" style="width:100px;" id="districtDdp1" onchange="getfilter('districtDdp1',2)"
