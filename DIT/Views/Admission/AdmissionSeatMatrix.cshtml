                                <span id="spnselectdropdown" class="text-danger" style="display:none;"></span>
                            </div>
                        </div>
                        <div class="col-lg-2">
                            <div id="" style="text-align:right;">
                                <a class=" btn btn-primary" id="btnsubmit" onclick="SubmitMainseatMatrix()">Submit</a>
                                <a class=" btn btn-primary" id="btnCancel" onclick="btnCancelseatmatrix()">Back</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    @*UseCase-30-Update Seat Matrix*@
