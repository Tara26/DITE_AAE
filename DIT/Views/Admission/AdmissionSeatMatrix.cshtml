@model Models.Admission.seatmatrixmodelNest
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    int sno = 1;
}

<link href="~/Scripts/Admission/MeritList.css" rel="stylesheet" />

<style>
    table.dataTable thead tr {
        background-color: cornflowerblue;
        color: white;
    }

    /*.taluk {
        display: none;
    }*/

    .buttons-html5 {
        border-radius: 5px;
        height: 40px;
        width: 100px;
        background: cornflowerblue;
    }

    .headetitle {
        text-align: center;
        background-color: cornflowerblue;
        color: white;
        border-radius: 5px;
        height: 35px;
    }

    .tooltip {
        position: relative;
        display: inline-block;
        border-bottom: 1px dotted black;
    }

    .tooltiptext {
        visibility: hidden;
        width: 200px;
        background-color: black;
        color: #fff;
        text-align: center;
        border-radius: 6px;
        padding: 5px 0;
        /* Position the tooltip */
        position: absolute;
        z-index: 1;
    }

    #viewbtn:hover .tooltiptext {
        visibility: visible;
    }

    .buttons-html5 {
        width: 180px;
    }

    table th {
        text-align: center;
        font-size: smaller;
    }
</style>
<br />
<br />
<br />

@if (Session["RoleId"].Equals(5))
{
    <ul class="nav nav-tabs font bg-light">
        <li>
            <a href="#tab_1" data-toggle="tab">View Seat Availability </a>
        </li>
        <li>
            <a href="#tab_2" data-toggle="tab">Generate Seat Matrix </a>
        </li>
