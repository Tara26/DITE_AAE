                                    <option value="">choose</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="col-sm-5">
                                <label>Taluk:</label>
                            </div>
                            <div class="col-sm-7">
                                <select class="form-control" style="width:100px;" id="talukDdp" onchange="getfilter('talukDdp',3)"
                                        @*onchange="GetTaluksData('talukDdp','instituteDdp','2')"*@>
                                    <option value="">choose</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-2">
                            <div class="col-sm-5">
                                <label>Institute:</label>
                            </div>
                            <div class="col-sm-7">
                                <select class="form-control" style="width:100px;" onchange="getfilter('instituteDdp',0)" id="instituteDdp"><option value="">choose</option></select>
                            </div>
                        </div>
                    </div>
                    @*<div class="row">
                            <div class="col-sm-12">
                                <button class="btn btn-primary" style="width:100px;float:right;" onclick="GetViewSeatAvailability()">Search</button>
                            </div>
                        </div>*@
