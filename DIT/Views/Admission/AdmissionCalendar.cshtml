<script type="text/javascript">
    var selectedTab = '@Model.selectTab';
    if (selectedTab != null)
    {
         if (selectedTab == 0) {
            $('.nav-tabs li:eq(0) a').tab('show')
        }
        else if (selectedTab == 1) {
            $('.nav-tabs li:eq(1) a').tab('show')
        }
        else if (selectedTab == 2) {
            $('.nav-tabs li:eq(2) a').tab('show')
        }

    }

    if ('@TempData["Saved"]' != "") {
        if ('@TempData["Saved"]' == "Saved") {
            var num = $('#notifNumber').val();
            //bootbox.alert("Calendar of events created Successfully  " +  num);
            bootbox.alert("</br> <b>Case Worker</b> has Created Calendar of events,  Notification No : <b>" + num + "</b> Successfully ");

            ClearFields();
            $('#courseType').val('');
            $('#notifNumber').val('');
            $('#NotifDesc').val('');

        }
        else if ('@TempData["Saved"]' == "Updated") {
             var num = $('#notifNumber').val();
           // bootbox.alert('Calendar of events Updated Successfully'+  num);
            bootbox.alert("</br> <b>Case Worker</b> has Updated Calendar of events, Notification No : <b>" + num + "</b> Successfully");
        }
        else if ('@TempData["Saved"]' == "Draft") {
             var num = $('#notifNumber').val();
            bootbox.alert("</br> <b>Case Worker</b> has Created Draft Calendar of events, Notification No : <b>" + num + "</b> Successfully");
        }
        else if ('@TempData["Saved"]' == "Failed") {
            bootbox.alert("</br> Failed to Create/Update data");
        }
        else if ('@TempData["Saved"]' == "Invalid") {
            bootbox.alert("</br> Some of the fields are invalid, please select valid data.");
        }
         else if ('@TempData["Saved"]' == "exist") {
            var num = $('#notifNumber').val();
            bootbox.alert("</br> Calendar Notification No : <b>" + num + "</b> already exists");
        }
    }

</script>

