    function SetNextFieldDate(date, numOfDays) {

        var selectedDate = new Date(date);
        var from = date.split("-")
        var selectedDate = new Date(from[2], from[1] - 1, parseInt(from[0]) + numOfDays);
        return selectedDate;
    }

