                $("#dvPassport").hide();
                $("#dvPassport1").show();
                var msg = "Please enter caste/category details manually";
                bootbox.alert(msg);
            }
        });
    });
    $(function () {
        $("input[name='chkeconomicsection']").click(function () {
            if ($("#chkYes").is(":checked")) {
                $("#dveconomic").show();
                $("#dveconomic1").show();
                $("#btnREWSRD").hide();
            } else {
                var msg = "Please enter Economically weeker section details manually";
                $("#dveconomic").hide();
                //bootbox.alert(msg);
                $("#dveconomic1").hide();
                $("#btnREWSRD").hide();
            }
        });
    });
    $(function () {
        $("input[name='chkHKregion']").click(function () {
            if ($("#chkYesHK").is(":checked")) {
                $("#dvHKR").show();
                $("#dvHKR1").show();
                $("#btnHYDKarRD").hide();
            } else {
                var msg = "Please enter Hyderabad Karnataka details manually";
                $("#dvHKR").hide();
                //bootbox.alert(msg);
                $("#dvHKR1").hide();
                $("#btnHYDKarRD").hide();
            }
        });
    });
    $(function () {
        $("input[name='chkDisability']").click(function () {
            if ($("#chkDyes").is(":checked")) {
                $("#divDisability").show();
