<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog" style="width:80%;">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-body">
                <a href="#" class="close" data-dismiss="modal" aria-label="close">&times;</a><br><br>
                <div class="accordion-option">
                    <h3 class="title">Application Form Details</h3>
                    <a href="javascript:void(0)" class="toggle-accordion active" accordion-id="#accordion"></a>
                </div>
                <div class="clearfix"></div>
                <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                    <div class="panel panel-default">
                        <div class="panel-heading" role="tab" id="headingOne">
                            <h3 class="panel-title">
                                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                    Applicant Details
                                </a>
                            </h3>
                        </div>
                        <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                            <div class="panel-body">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label>Select the Board type</label>
                                        </div>
                                        <div class="col-md-5">
                                            <input type="radio" id="RStateBoardType" name="RStateBoardType" value="1" checked onchange="OnChangeStateBoardType()" disabled>
                                            <label>State Board</label>&nbsp;&nbsp;
                                            <input type="radio" id="RStateBoardType" name="RStateBoardType" value="0" onchange="OnChangeStateBoardType()" disabled>
                                            <label>Other Boards</label>
                                        </div>
                                    </div>
                                    <br>
                                    <div id="GetAppliedBasicsForBoardType" class="row">
                                        <div class="col-md-5">
                                            <label>Applied for which basics<font class="text-danger">*</font></label>
                                        </div>
                                        <div class="col-md-5">
                                            @foreach (var Qlist in ViewBag.QualificationList)
                                            {
                                                <input type="radio" name="RAppBasics" disabled style="width:5%" onchange="OnChangeGetAppliedBasicsForBoardType()" value="@Qlist.QualificationId" checked> <b> @Qlist.Qualification </b>
                                            }
                                        </div>
                                    </div>
                                    <br>
                                    <div id="GetRollNumberFor10thStd" class="row">
                                        <div class="col-md-5">
                                            <label>Enter the Roll Number/Registration Number for 10th Standard<font class="text-danger">*</font></label>
                                        </div>
